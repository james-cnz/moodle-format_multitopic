{{!
    @template format_multitopic/courseformat/contenttabs/tabtreecontainer

    Tab tree comtainer.

    Example context (json):
    {
        "tabs": [{
            "active": "true",
            "inactive": "false",
            "link": [{
                "link": "http://moodle.org"
            }],
            "title": "Moodle.org",
            "text": "Moodle community"
        }]
    }
}}
<div id="course-tabs-{{uniqid}}" class="course-section-tabs">
    {{> format_multitopic/courseformat/contenttabs/tabtree}}
</div>
{{#js}}
require(['format_multitopic/courseformat/contenttabs/tabtreecontainer'], function(component) {
    component.init('#course-tabs-{{uniqid}}');
});
{{/js}}
